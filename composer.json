{
    "name": "fredbradley/socs-php",
    "description": "A PHP wrapper for the SOCS XML feeds.",
    "keywords": [
        "fredbradley",
        "socs-php"
    ],
    "homepage": "https://github.com/fredbradley/socs-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Bradley",
            "email": "code@fredbradley.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4||^8.0",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^6.3||^7.3",
        "illuminate/collections": "^7.30||^8.53",
        "nesbot/carbon": "^2.31",
        "vlucas/phpdotenv": "^4.0||^5.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.0",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "FredBradley\\SOCS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FredBradley\\SOCS\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm --show-info=true",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
